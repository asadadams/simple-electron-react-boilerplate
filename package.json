{
  "name": "electronreactboilerplate",
  "productName": "Electron React Boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate by Asad",
  "main": "main.js",
  "scripts": {
    "dev": "webpack --watch",
    "start": "electron .",
    "test": "jest --watchAll",
    "package-win-32": "electron-packager . desktopApp --overwrite --asar --platform=win32 --arch=ia32 --ignore=build/installer --ignore=build/release-builds --ignore=__tests__ --ignore=__tests__/__snapshots__ --icon=build/icon.ico --prune=true --out=build/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"desktopApp\"",
    "package-win-64": "electron-packager . desktopApp --overwrite --asar --platform=win32 --arch=x64 --ignore=build/installer --ignore=build/release-builds --ignore=__tests__ --ignore=__tests__/__snapshots__ --icon=build/icon.ico --prune=true --out=build/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"desktopApp\"",
    "package-win": "npm run package-win-32 & npm run package-win-64",
    "package-mac": "electron-packager . desktopApp --overwrite --asar --platform=darwin --arch=ia32 --ignore=build/installer --ignore=build/release-builds --icon=build/icon.ico --prune=true --out=build/release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"desktopApp\"",
    "create-installer-win-32": "node createinstaller-win-32.js",
    "create-installer-win-64": "node createinstaller-win-64.js",
    "create-installer-win": "npm run create-installer-win-32 & npm run create-installer-win-64"
  },
  "repository": "https://github.com/asadadams/simple-electron-react-boilerplate",
  "author": "Asad Adams",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-import": "^1.6.5",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.9",
    "electron": "^15.5.5",
    "electron-reload": "^1.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "history": "3.0.0",
    "react-router": "^5.1.2",
    "react-scripts": "1.1.0",
    "style-loader": "^0.20.2",
    "warning": "3.0.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-jest": "16.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "electron-reload": "^1.2.2",
    "jest": "^23.1.0",
    "jest-css-modules": "^2.1.0",
    "jest-css-modules-transform": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "1.1.0",
    "electron-packager": "^14.1.1",
    "electron-winstaller": "^2.6.4"
  }
}